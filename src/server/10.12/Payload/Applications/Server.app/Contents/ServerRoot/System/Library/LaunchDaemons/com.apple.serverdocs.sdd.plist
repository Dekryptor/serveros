	<string>_xserverdocs</string>
	<key>SoftResourceLimits</key>
	<dict>
		<key>NumberOfFiles</key>
		<integer>10240</integer>
	</dict>
	<key>EnvironmentVariables</key>
	<dict>
		<key>SDD_SANDBOX</key>
		<string>/Applications/Server.app/Contents/ServerRoot/System/Library/Sandbox/com.apple.serverdocs.sdd.sb</string>
	</dict>
