<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>MachServices</key>
	<dict>
		<key>com.apple.serverdocs.sdd</key>
		<true/>
		<key>com.apple.serverdocs.sdd.c</key>
		<true/>
	</dict>
	<key>PreventsSleep</key>
	<true/>
	<key>UnloadOnSleep</key>
	<true/>
	<key>Label</key>
	<string>com.apple.serverdocs.sdd</string>
