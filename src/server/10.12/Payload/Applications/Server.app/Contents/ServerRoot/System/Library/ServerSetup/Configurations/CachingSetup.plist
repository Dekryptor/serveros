				<key>group</key>
				<string>wheel</string>
				<key>mode</key>
				<string>0755</string>
			</dict>
			<dict>
				<key>path</key>
				<string>${serverLibrary}/Caching</string>
				<key>owner</key>
				<string>_assetcache</string>
				<key>group</key>
				<string>_assetcache</string>
				<key>mode</key>
				<string>0775</string>
			</dict>
			<dict>
				<key>path</key>
				<string>${serverLibrary}/Caching/Data</string>
				<key>owner</key>
				<string>_assetcache</string>
				<key>group</key>
				<string>_assetcache</string>
				<key>mode</key>
				<string>0750</string>
			</dict>
		</array>
	</dict>

	<key>migration</key>
	<dict>
		<key>directories</key>
		<array>
			<dict>
				<key>path</key>
				<string>${serverLibrary}</string>
				<key>owner</key>
