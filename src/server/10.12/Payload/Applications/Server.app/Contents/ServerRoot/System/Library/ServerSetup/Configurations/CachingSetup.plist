<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>description</key>
	<string>Caching Service</string>

	<key>promotion</key>
	<dict>
		<key>directories</key>
		<array>
			<dict>
				<key>path</key>
				<string>${serverLibrary}</string>
				<key>owner</key>
