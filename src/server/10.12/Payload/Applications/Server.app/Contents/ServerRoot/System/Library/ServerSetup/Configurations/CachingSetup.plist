				<string>0775</string>
			</dict>
			<dict>
				<key>path</key>
				<string>${serverLibrary}/Caching/Data</string>
				<key>owner</key>
				<string>_assetcache</string>
				<key>group</key>
				<string>_assetcache</string>
				<key>mode</key>
				<string>0750</string>
			</dict>
		</array>
	</dict>

	<key>postSetup</key>
	<dict>
		<key>scripts</key>
		<array>
			<string>${serverRoot}/usr/libexec/AssetCache/CachingSetup.sh</string>
		</array>
	</dict>
</dict>
</plist>
