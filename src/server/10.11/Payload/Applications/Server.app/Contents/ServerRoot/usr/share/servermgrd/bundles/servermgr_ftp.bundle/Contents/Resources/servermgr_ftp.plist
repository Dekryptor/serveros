<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>description</key>
	<string>Perform setup for FTP service</string>
	<key>dependencies</key>
	<array/>
	<key>environment</key>
	<dict/>
	<key>promotion</key>
	<dict>
		<key>scripts</key>
		<array>
			<dict>
				<key>path</key>
				<string>/bin/launchctl</string>
				<key>arguments</key>
				<array>
					<string>unload</string>
					<string>-w</string>
					<string>/System/Library/LaunchDaemons/ftp.plist</string>
				</array>
			</dict>
		</array>
	</dict>
</dict>
</plist>
