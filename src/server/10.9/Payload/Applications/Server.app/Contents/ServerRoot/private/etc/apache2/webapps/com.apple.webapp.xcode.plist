<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>name</key>
		<string>com.apple.webapp.xcode</string>
		<key>includeFiles</key>
		<array>
			<string>/Library/Server/Web/Config/apache2/httpd_xcsweb_xcode.conf</string>
		</array>
		<key>proxies</key>
		<dict>
			<key>/xcs</key>
			<dict>
				<key>path</key>
				<string>/xcs</string>
				<key>urls</key>
				<array>
					<string>http://localhost:4444/xcs</string>
				</array>
			</dict>
			<key>/xcs/svc</key>
			<dict>
				<key>path</key>
				<string>/xcs/svc</string>
				<key>urls</key>
				<array>
					<string>http://localhost:4444/svc</string>
				</array>
			</dict>
			<key>/xcs/streams/activity</key>
			<dict>
				<key>path</key>
				<string>/xcs/streams/activity</string>
				<key>urls</key>
				<array>
					<string>http://localhost:4444/streams/activity</string>
				</array>
			</dict>
			<key>/xcode/files/download</key>
			<dict>
				<key>path</key>
				<string>/xcode/files/download</string>
				<key>urls</key>
				<array>
					<string>http://localhost:4444/files</string>
				</array>
			</dict>
			<key>/xcode</key>
			<dict>
				<key>path</key>
				<string>/xcode</string>
				<key>urls</key>
				<array>
					<string>http://localhost:4444/app-context/xcode</string>
				</array>
			</dict>
		</dict>
		<key>requiredModuleNames</key>
		<array>
			<string>proxy_module</string>
			<string>headers_module</string>
		</array>
		<key>requiredWebAppNames</key>
		<array>
			<string>com.apple.webapp.collabd</string>
			<string>com.apple.webapp.auth</string>
			<string>com.apple.webapp.xcsbuildd</string>
		</array>
		<key>launchKeys</key>
		<array>
			<string>com.apple.collabd.scm_poller</string>
			<string>com.apple.xcs.stats</string>
			<!-- collabd, xcscred and xcscredhandle are running all the time (managed by serverd) -->
		</array>
		<key>sslPolicy</key>
		<integer>4</integer> <!-- 4: UseSSLAndNonSSL -->
	</dict>
</plist>
