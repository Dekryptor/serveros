<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Disabled</key>
	<true/>
	<key>Label</key>
	<string>com.apple.Rooms</string>
	<key>KeepAlive</key>
	<true/>
	<key>ProgramArguments</key>
    <array>
        <string>/Applications/Server.app/Contents/ServerRoot/usr/libexec/jabberd/Rooms</string>
        <string>-c</string>
        <string>/Library/Server/Messages/Config/Rooms.plist</string>
    </array>
	<key>UserName</key>
	<string>_jabber</string>
	<key>GroupName</key>
	<string>daemon</string>
    <key>StandardErrorPath</key>
    <string>/dev/null</string>
    <key>StandardOutPath</key>
    <string>/dev/null</string>
    <key>PreventsSleep</key>
    <true/>
</dict>
</plist>
