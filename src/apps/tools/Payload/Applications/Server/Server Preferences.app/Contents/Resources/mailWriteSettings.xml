		</dict>
		<key>imap</key>
		<dict>
			<key>enable_pop</key>
			<integer>1</integer>
			<key>enable_imap</key>
			<integer>1</integer>
		</dict>
	</dict>
	<key>optionalArguments</key>
	<dict>
		<key>parameters</key>
		<array>
			<string>imap:enable_pop</string>
			<string>imap:enable_imap</string>
