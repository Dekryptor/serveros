	<string>writeSettings</string>
	<key>configuration</key>
	<dict>
		<key>postfix</key>
		<dict>
			<key>enable_smtp</key>
			<integer>1</integer>
			<key>enable_smtp_in</key>
			<integer>1</integer>
			<key>enable_smtp_out</key>
			<integer>1</integer>
			<key>relayhost_enabled</key>
			<false/>	
			<key>relayhost</key>
			<string></string>
			<key>smtp_sasl_auth_enable</key>
